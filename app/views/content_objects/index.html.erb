<div id="content_list">
  <% if @selected_category %>
    <p>
      <strong>Currently Viewing: <%=@selected_category.name %></strong>
    </p>
  <% end %>
  <ul> 
    <% @content_objects.each do |content_object| %>
      <li <%= (content_object == @content_objects.last) ? ' class="last" ' : '' %>>
        <%= link_to(content_object.name, content_object_path(content_object)) %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate @content_objects %>
</div>